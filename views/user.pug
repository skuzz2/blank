extends layout
block content
  p Hello #{username}
  table
    tr(id='tableHeader')
        th Username
        th Password
        th User Level
        th Email
        th Age
        th Question #1
        th Question #2
        th Question #3
        th
          tr
            td #{username}
            td #{password}
            td #{admin}
            td #{email}
            td #{age}
            td #{answerOne}
            td #{answerTwo}
            td #{answerThree}
            td
              a(href='/edit/' + id) Edit
  a(href='/logout') Logout