extends layout

block content
  p Hello #{item.username}
  
  tr(id='tableHeader')
      th Id
      th Username
      th Password
      th User Level
      th Email
      th Age
      th Question #1
      th Question #2
      th Question #3
      th 
        tr
          td #{item.username}
          td #{item.password}
          td #{item.admin}
          td #{item.email}
          td #{item.age}
          td #{item.answerOne}
          td #{item.answerTwo}
          td #{item.answerThree}
          td
            a(href='/delete/' + item.id) Delete Account
          td
            a(href='/edit/' + item.id) Edit Info